<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.mybatis_plus.dao.ProductCodeMapper">

    <!-- 开启二级缓存 -->
<!--    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.springboot.mybatis_plus.domain.ProductCode">
        <id column="product_key" property="productKey" />
        <result column="tag" property="tag" />
        <result column="code" property="code" />
        <result column="count" property="count" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        product_key, tag, code, count
    </sql>
<select id="selectList" resultType="ProductCode">
    select 
    <include refid="Base_Column_List"></include>
     from product_code
</select>
</mapper>
